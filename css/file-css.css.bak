}
/*4.store-product*/
.store-product{
	position:relative;
	background-color: beige;
}
.store-prd{
	width:250px;
	display:inline-block;
	padding:15px;
    overflow: hidden;
}
.store-img > a > img{
	width:100%;
    margin-bottom: 30px;
}
.store-detail{
    position: relative;
	text-align: left;
    background-color: #3f3530;
    padding: 20px;
}
.store-rate{
	position: absolute;
    font-size: 20px;
    display: inline-block;
    background-color: saddlebrown;
    padding: 8px 15px;
    top: -20px;
    left: 15px;
    border-radius: 8px;
    text-align: center;
    color: #fff;
    font-weight: bold;
}
.store-rate-del{
	position: absolute;
    font-size: 20px;
    display: inline-block;
	text-decoration:line-through;
    background-color: #5c4543;
    padding: 8px 15px;
    top: -20px;
    right: 15px;
    border-radius: 8px;
    text-align: center;
    color: #fff;
    font-weight: bold;
}
.store-detail > h3{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.store-detail > h3 > a{
    color: wheat;
}
.store-detail > h3 > a:hover{
	color: #e6a391;
}
.store-detail > p{
	color: khaki;
}
.store-price{
	position:relative;
	font-size:0;
}
.store-price > a{
	display: block;
    width: 50%;
    float: left;
    text-align: center;
    padding: 15px 0;
    font-size: 18px;
	color:#fff;
	background-color: #dc8068;
}
.store-price > a:first-child{
	color:#e6a391;
	background-color: #5c4543;
}
.store-price > a:hover{
	background-color: #c94f2f;
}
.store-price > a:first-child:hover{
    background-color: #886663;
}
.product-view-more{
	text-align:center;
    margin: 11px 0 50px;
}
.product-view-more > a{
	padding: 10px 20px;
    font-size: 16px;
    font-family: "glober", sans-serif;
    font-weight: 400;
    border: 1px solid #dc8068;
    color: #fff;
    background: #dc8068;
    text-transform: uppercase;
}
.noi-dung-chi-tiet-01{
	color: papayawhip;
    font-size: 18px;
	max-height: 275px;
    overflow: hidden;
    text-overflow: ellipsis;
}
.produc-detail-hover{
	background: rgba(54,46,41,0.9);
	width:100%;
	height: 581px;
    position: absolute;
	bottom: 0;
	color:papayawhip;
	font-size:18px;
	display:none;
}
.store-price:hover .produc-detail-hover{
	display:block;		
}
.product-view-more > a:hover{
	background-color:#fff;
	color: #dc8068;
}
p.mo-ta-chi-tiet{
	background-color:lightsalmon;
    padding: 15px 0;
	margin:0;
}
p.tieu-de-chi-tiet{
	margin: 10px 0 20px;
