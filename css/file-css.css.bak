body{
	margin:0;
    text-align: center;
	min-width:480px !important;
	width:auto;
}
a {
	text-decoration:none;
}
ul > li{
	list-style-type:none;
}
::-webkit-scrollbar { 
    display: none; 
}
/*1.slide*/
.slide-top-img{
	width:100%;
	height:100%;
}
.header{
	background-color: rgba(54,44,55,0.9);
    position: fixed;
    width: 100%;
    z-index: 999;
}
a.logo-header{
	padding:12px 0 8px !important;
	width:230px;
}
a.logo-header:hover{
	background:none !important;
}
a.logo-header > img{
	width:165px;
	height:62px;
}
.header li{
	float: left;
    min-width: 109px;
    text-align: center;
}
.header li > img{
	width: 160px;
    height: 70px;
	margin-top: 10px;
    margin-left: 50px;
    margin-right: 35px;
}
.header li > a{
	font-family: "glober",sans-serif;
	color:aliceblue;
	display: block;
    padding: 34px 8px;
    font-weight: bold;
}
.header li > a:hover{background:#dc8068;}
/*2.menu*/
.menu{
	position:relative;
    background-color: honeydew;
}
.menu > .menu-container{
	position: relative;
	margin:15px 75px;
	overflow: hidden;
}
.menu-container > .banner-qc{
	position:relative;
	background-color:#50aecc;
	overflow: hidden;
    padding: 25px 40px 25px 15px;
}
.banner-qc > img{
	float:right;
}
.banner-qc > h3, h4{
	position:absolute;
	display:inline-block;
	color: #fff;
    font-family: 'Spirax', cursive;
	left:20px;
}
.banner-qc > h3{
	top:50px;
    font-size: 30px;
    margin: 0;
}
.banner-qc > h4{
	bottom:15px;
    font-size: 60px;
    margin-bottom: 30px;
    margin-top: 25px;
}
.banner-qc > h4 > i{
	display:inline-block;
	margin-right:15px;
}
.menu-buider{
	display:inline-block;
	position: relative;
	float:left;
    min-height: 1px;
    width:30%;
    padding: 25px 15px;
    background: #f1c152;
    color: #362e29;
}
p.menu-bui1{
	font-size: 30px;
    font-family: 'Spirax', cursive;
    margin: 0;
}
p.menu-bui2{
	font-size: 16px;
    font-style: italic;
    margin-bottom: 0;
}
.menu-stack1{
	display: inline-block;
    width: 60%;
    background-color: #3f3530;
    padding: 20px;
	min-height:445px;
	max-height:445px;
	overflow:auto;
}
.menu-st-right, .menu-st-left{
	display: inline-block;
    width: 45%;
    padding: 10px 15px;
}
.menu-st-left{
	border-right: 1px dotted #56453f;
}
.menu-st1{
    width: 100%;
    text-align: left;
    border-bottom: 1px dotted #56453f;
    display: inline-block;
}
.menu-st1 > a{
    color: #b9a39a;
	font-size:30px;
	font-family: 'Spirax', cursive;
}
p.menu-st1-detail{
    color: #9a847b;
    margin-bottom: 10px;
	font-size: 16px;
    font-style: italic;
    margin-top: 10px;
}
p.menu-st1-price{
	color: #b9a39a;
	font-size:30px;
	font-family: 'Spirax', cursive;
	margin: 8px 0 20px;
}
.menu-st1-price > span{
	margin-left: 25px;
    color: #dc8068;
    font-size: 18px;
    text-decoration: line-through;
}
.container-stack2{
	position: relative;
	display:inline-block;
	width: 30%;
    height: 420px;
	margin: 30px;
    margin-left: 0;
	margin-bottom: 60px;
}
.container-stack2-last{
	margin-right:0;
}
.menu-stack2{
	position: absolute;
    background-color: #68d0af;
    width: 100%;
    height: 100%;
    padding: 15px 0;
	left:0;
	top:0;
}
.menu-stack2-second{
	background-color: #d2d86f;
}   
.menu-stack2-last{
	background-color: #83bed7;
}
.menu-stack2 > img{
	width:45%;
	transition: all 1s ease;
	-webkit-transition: all 1s ease;
	-moz-transition: all 1s ease;
	-o-transition: all 1s ease;
}
.menu-stack2 > img:hover{
	width:55%;
	animation-duration: 0.2s;
	transform: scale(1,1);
	-webkit-transform: scale(1,1);
	-moz-transform: scale(1,1);
	-o-transform: scale(1,1);
	-ms-transform: scale(1,1);
}
.menu-stack2 > p.p-stao{
    position: absolute;
    bottom: 120px;
    left: 100px;
	color:#333;
	font-size: 60px;
	margin:0;
    line-height: 55px;
}
.menu-stack2 > p.p-hv{
    position: absolute;
    bottom: 85px;
    left: 120px;
	color:#333;
	font-size: 30px;
    font-family: 'Spirax', cursive;
	margin: 0;
}
.menu-stack2 > a{
    position: absolute;
    bottom: 30px;
    left: 140px;
	display: inline-block;
    padding: 8px 10px;
    color: black;
    border: 1px solid currentColor;
}
.menu-stack2 > a:hover{
	background-color: rgb(220, 128, 104);
	color:#fff;
	border:none;
}
.menu-content, .menu-content-2{
	position:relative;
}
ul.nav-menu-content, .nav-menu-content-2{
	padding:0;
	margin:0;
	position:absolute;
	display: none;
	background-color: rgba(54,44,55,0.9);
}
.nav-menu-content-2{
	left: 109px;
    top: 0;
}
ul.nav-menu-content > li{	
	float:none;
}
ul.nav-menu-content > li > a{
	padding: 15px 8px;
}
ul.nav-menu-content-2 > li > a{
	padding: 15px 8px;
}
.menu-content:hover ul.nav-menu-content{
	display:block;
}
.menu-content-2:hover ul.nav-menu-content-2{
	display:block;
}
ul.header-ul{
	margin: 0;
}
.header > a{
	padding: 55px 25px 15px;
    margin-right: 5px;
}
.header > a > img{
    margin-top: 30px;
    width: 20px;
}
a.menu-repon{
	display:none;
}
/*gio hang*/
.cart-hover{
	display:none;
	position:absolute;
	font-size:15px;
	background-color:#fff;
	right: 275px;
    padding: 10px;
    top: 86px;
	z-index:999;
}
a.cart div.cart-hover::after{
	content:" ";
	border:solid;
	border-width:10px;
	border-color:transparent transparent #fff transparent;
	position:absolute;
    right: 43%;
    bottom: 100%;
}
a.cart:hover .cart-hover{
	display:block;
}
/*search*/
.search-bar{
	position:absolute;
	text-align:left;
    right:120px;
	display:none;
    background-color: rgba(54,44,55,0.9);
	top: 86px;
}
.search-bar > p{
	color:#fff;
	font-size:22px;
    margin-left: 15px;
}
.search-bar > input[type="text"]{
    font-size: 17px;
    padding: 15px;
    width: 282px;
	z-index:99;
}
#hide{
	position: absolute;
    top: 20px;
    right: 5px;
	background-color: transparent;
    border: none;
    color: #fff;
    font-size: 25px;
}
/*user*/
.login{
    display: none;
    position: absolute;
    background-color: #fff;
    padding: 15px;
    top: 86px;
    right: 140px;
	text-align: left;
	font-size: 18px;
}
p.log-p1, p.log-p4{
	text-align:center;
}
p.log-p2 > label{
	margin-right:25px;
}
.login::after{
	content:" ";
	border:solid;
	border-width:10px;
	border-color:transparent transparent #fff transparent;
	position:absolute;
    right: 46%;
    bottom: 100%;
}
a.user-login:hover .login{
	display:block;
}
/*3.top-banner*/
.top-banner1{
	background-image:url("../image/coffee-banner.jpg");
	background-repeat:no-repeat;
	background-size: 100% 700px;
    height: 700px;
    margin-top: 60px;
}
.banner1-left{
	float:left;
    width: 40%;
}
.banner1-left > img{
	height:630px;
}
.banner1-right{
	float:right;
	width:48%;
    margin-right: 75px;
    text-align: right;
    color: aliceblue;
}
p.banner1-p1{
	font-size: 40px;
	margin-top: 200px;
}
p.banner1-p2{
	font-size: 18px;
	font-style:italic;
}
.banner1-right > a{
	display:inline-block;
	padding:12px 15px;
	text-transform:uppercase;
	border: 1px solid #dc8068;
    color: #fff;
